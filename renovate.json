{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "extends": [
      "config:base", ":disableDependencyDashboard", "github>whitesource/merge-confidence:beta"
    ],
    "packageRules": [
      {
        "matchPackageNames": [
          "com.github.spotbugs:spotbugs-maven-plugin",
          "github-action"
        ],
        "matchUpdateTypes": [
          "minor",
          "patch",
          "pin",
          "digest"
        ],
        "automerge": true
      }
    ]
  
  }